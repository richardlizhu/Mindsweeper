{
  "name": "Hello World!",
  "version": "1.0",
  "description": "My first Chrome extension.",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
"content_scripts": [
  {
    "matches": [
      "https://en.wikipedia.org/*"
    ],
    "js": ["content.js"]
  }
],
 "permissions": [
    "https://*/",
    "storage"
  ]
}